package test;
import java.util.*;
public class firstNonRepeating {
	public static Scanner sc=new Scanner(System.in);
	public static class Node
	{
		char a;
		Node next,prev;
		Node(char a)
		{
			this.a=a;
			next=null;
			prev=null;
		}
	}
	public static Node head=null;
	public static Node tail=null;
	public static void insertInCharStream(char ch,Node[]ascii)
	{
		if(ascii[ch].a==' ')
			System.out.println("Character already repeated");
		else if(ascii[ch]==null)
		{
			Node temp=new Node(ch);
			ascii[ch]=temp;
			if(head===null)
			{
				head=temp;
				tail=head.next;
			}
			else
			{
				while(head.next!=null)
					head=head.next;
				head.next=
			}
			
		}
		
	}
	public static void main(String args[])
	{
		while(true)
		{
			System.out.println("Enter 1 to insert char in the stream");
			int a;
			Node[] ascii=new Node[256];
			a=sc.nextInt();
			System.out.println();
			if(a==1)
			{
				char ch;
				System.out.println("Enter the character");
				ch=sc.next().charAt(0);
				insertInCharStream(ch,ascii);
				
				
			}
			
		}
	}
	
	
	
}
